{
  "version": 2,
  "settings": {
    "custom": {
      "colorSchemes": [ "primary", "musician", "secondary" ]
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "color": {
      "palette": [
        { "slug": "primary", "color": "#0057B8", "name": "Primary Blue" },
        { "slug": "musician", "color": "#A83279", "name": "Musician Purple" },
        { "slug": "secondary", "color": "#4CAF50", "name": "Secondary Green" },
        { "slug": "light-gray", "color": "#F5F5F5", "name": "Light Gray" },
        { "slug": "white", "color": "#FFFFFF", "name": "White" },
        { "slug": "black", "color": "#000000", "name": "Black" },
        { "slug": "accent-yellow", "color": "#F7C948", "name": "Accent Yellow" }
      ],
      "background": true,
      "text": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Museo Sans, sans-serif",
          "name": "Museo Sans",
          "slug": "museo-sans"
        }
      ],
      "fontSizes": [
        { "slug": "small", "size": "0.875rem", "name": "Small" },
        { "slug": "normal", "size": "1rem", "name": "Normal" },
        { "slug": "medium", "size": "1.25rem", "name": "Medium" },
        { "slug": "large", "size": "1.5rem", "name": "Large" },
        { "slug": "x-large", "size": "2.25rem", "name": "Extra Large" },
        { "slug": "xx-large", "size": "3rem", "name": "XX-Large" }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "museo-sans",
      "fontSize": "1rem",
      "lineHeight": "1.6"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "museo-sans",
          "lineHeight": "1.2"
        }
      },
      "h1": {
        "typography": { "fontSize": "3rem", "fontWeight": "700" }
      },
      "h2": {
        "typography": { "fontSize": "2.25rem", "fontWeight": "500" }
      },
      "h3": {
        "typography": { "fontSize": "1.5rem", "fontWeight": "500" }
      },
      "h4": {
        "typography": { "fontSize": "1.5rem", "fontWeight": "500" }
      },
      "h5": {
        "typography": { "fontSize": "1.5rem", "fontWeight": "500" }
      },
      "h6": {
        "typography": { "fontSize": "1.5rem", "fontWeight": "500" }
      }
    }
  }
}